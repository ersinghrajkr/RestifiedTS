{
  "environment": "development",
  "services": {
    "userService": {
      "baseURL": "http://localhost:3001",
      "timeout": 5000,
      "retries": 2,
      "headers": {
        "Content-Type": "application/json",
        "X-Environment": "development"
      }
    },
    "paymentService": {
      "baseURL": "http://localhost:3002",
      "timeout": 8000,
      "retries": 3,
      "headers": {
        "Content-Type": "application/json",
        "X-Environment": "development"
      }
    },
    "notificationService": {
      "baseURL": "http://localhost:3003",
      "timeout": 3000,
      "retries": 1,
      "headers": {
        "Content-Type": "application/json",
        "X-Environment": "development"
      }
    },
    "orderService": {
      "baseURL": "http://localhost:3004",
      "timeout": 6000,
      "retries": 2,
      "headers": {
        "Content-Type": "application/json",
        "X-Environment": "development"
      }
    }
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "database": "restified_dev",
    "username": "dev_user",
    "password": "dev_password",
    "ssl": false,
    "pool": {
      "min": 2,
      "max": 10
    }
  },
  "authentication": {
    "jwt": {
      "secret": "dev-secret-key",
      "expiresIn": "24h"
    },
    "oauth": {
      "clientId": "dev-client-id",
      "clientSecret": "dev-client-secret",
      "tokenUrl": "http://localhost:3000/oauth/token"
    }
  },
  "logging": {
    "level": "debug",
    "format": "detailed",
    "includeRequestBody": true,
    "includeResponseBody": true
  },
  "testing": {
    "parallel": true,
    "maxConcurrency": 5,
    "timeout": 30000,
    "retries": 1
  }
}